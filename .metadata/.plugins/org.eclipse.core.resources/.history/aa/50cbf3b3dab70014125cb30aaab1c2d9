package game;

public class Block {
	BlockType blockType;
	
	public enum BlockType {
	    WALL, GOAL, PLAYER, VOID
	}
	
	public Block(BlockType blockType) {
		this.blockType = blockType;
	}
	
	public void printBlock() {
		switch (this.blockType) {
		case WALL:
			break;
		case GOAL:
			break;
		case PLAYER:
			break;
		case VOID:
			break;
		}
	}

}
